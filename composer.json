{
    "name": "exads-exercises/php",
    "description": "Exercises PHP",
    "type": "project",
    "authors": [
        {
            "name": "Mickael Marques",
            "email": "mickaelmarques14@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "ExadsExercises\\": "src/"
        }
    },
    "scripts": {
        "clear-cache-command": [
            "composer clearcache",
            "composer dump-autoload"
        ],
        "description": "Run to dumping autoload and clearing cache"
    },
    "require": {
        "doctrine/orm": "^3.0",
        "symfony/console": "^7.0",
        "symfony/dotenv": "^7.0"
    }
}
